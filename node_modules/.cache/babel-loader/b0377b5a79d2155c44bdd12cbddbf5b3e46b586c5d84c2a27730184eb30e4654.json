r\nimport Teacher_img from '../../images/teacher.png';\r\n\r\nclass Section4 extends React.Component{\r\n    render(){\r\n        return(\r\n            <section className=\"Section4\">\r\n                <div className=\"Section4-block_center\">\r\n                    <h1 className=\"Section4-h1\">Наши преподаватели</h1>\r\n                    <p className=\"Section4-text\">Мы проводим строгий отбор на роль преподавателя <br/> и приглашаем на работу только лучших специалистов</p>\r\n\r\n                    <div className=\"Section4-block\">\r\n                        <div className=\"Section4-picture_teacher\">\r\n                            <img src={Teacher_img} alt=\"\" />\r\n                        </div>\r\n\r\n                        <div className=\"Section4-description_teacher_1\">\r\n                            <h2 className=\"Section4-h2_1\"><span className=\"Section4-h2_text\">и еще</span> <br/> <b className=\"Section4-b\">30+</b></h2>\r\n                            <p className=\"Section4-block_p_1\">талантливых преподавателей с опытом работы с детьми и подростками</p>\r\n                        </div>\r\n\r\n                        <div className=\"Section4-description_teacher_2\">\r\n                            <h2 className=\"Section4-h2_1\">Учитывают детскую психологию</h2>\r\n                            <p className=\"Section4-block_p_2\">Подстроим программу курса под интересы ученика. Установим эмоциональный контакт с учеником в первые минуты урока.</p>\r\n                        </div>\r\n\r\n                        <div className=\"Section4-description_teacher_3\">\r\n                            <h2 className=\"Section4-h2_1\">Молодые и талантливые</h2>\r\n                            <p className=\"Section4-block_p_2\">Преподаватели легко объяснят, поддержат и вдохновят развиваться дальше через личный пример.</p>\r\n                        </div>\r\n\r\n                        <div className=\"Section4-description_teacher_4\">\r\n                            <h2 className=\"Section4-h2_2\">12 лет</h2>\r\n                            <p className=\"Section4-block_p_2\">учим и развиваем наших учеников. Имеем большой опыт в детском образовании </p>\r\n                        </div>\r\n\r\n                        <div className=\"Section4-description_teacher_5\">\r\n                            <h2 className=\"Section4-h2_2\">1900+</h2>\r\n                            <p className=\"Section4-block_p_2\">учеников прошли через наши занятия и остались довольны</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                \r\n            </section>\r\n        )\r\n    }\r\n}\r\nexport default Section4;"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,2BAA2B,CAClC,MAAO,CAAAC,WAAW,KAAM,0BAA0B,CAAC,OAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,yBAEnD,KAAM,CAAAC,QAAQ,QAAS,CAAAN,KAAK,CAACO,SAAS,CAClCC,MAAMA,CAAA,CAAE,CACJ,mBACIL,IAAA,YAASM,SAAS,CAAC,UAAU,CAAAC,QAAA,cACzBL,KAAA,QAAKI,SAAS,CAAC,uBAAuB,CAAAC,QAAA,eAClCP,IAAA,OAAIM,SAAS,CAAC,aAAa,CAAAC,QAAA,CAAC,yGAAkB,CAAI,CAAC,cACnDL,KAAA,MAAGI,SAAS,CAAC,eAAe,CAAAC,QAAA,EAAC,+PAAgD,cAAAP,IAAA,QAAI,CAAC,4QAAkD,EAAG,CAAC,cAExIE,KAAA,QAAKI,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eAC3BP,IAAA,QAAKM,SAAS,CAAC,0BAA0B,CAAAC,QAAA,cACrCP,IAAA,QAAKQ,GAAG,CAAEV,WAAY,CAACW,GAAG,CAAC,EAAE,CAAE,CAAC,CAC/B,CAAC,cAENP,KAAA,QAAKI,SAAS,CAAC,gCAAgC,CAAAC,QAAA,eAC3CL,KAAA,OAAII,SAAS,CAAC,eAAe,CAAAC,QAAA,eAACP,IAAA,SAAMM,SAAS,CAAC,kBAAkB,CAAAC,QAAA,CAAC,2BAAK,CAAM,CAAC,IAAC,cAAAP,IAAA,QAAI,CAAC,IAAC,cAAAA,IAAA,MAAGM,SAAS,CAAC,YAAY,CAAAC,QAAA,CAAC,KAAG,CAAG,CAAC,EAAI,CAAC,cAC1HP,IAAA,MAAGM,SAAS,CAAC,oBAAoB,CAAAC,QAAA,CAAC,gWAAiE,CAAG,CAAC,EACtG,CAAC,cAENL,KAAA,QAAKI,SAAS,CAAC,gCAAgC,CAAAC,QAAA,eAC3CP,IAAA,OAAIM,SAAS,CAAC,eAAe,CAAAC,QAAA,CAAC,gKAA4B,CAAI,CAAC,cAC/DP,IAAA,MAAGM,SAAS,CAAC,oBAAoB,CAAAC,QAAA,CAAC,wlBAAiH,CAAG,CAAC,EACtJ,CAAC,cAENL,KAAA,QAAKI,SAAS,CAAC,gCAAgC,CAAAC,QAAA,eAC3CP,IAAA,OAAIM,SAAS,CAAC,eAAe,CAAAC,QAAA,CAAC,sHAAqB,CAAI,CAAC,cACxDP,